{
	"name": "clubs-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prettier": "pnpm recursive run prettier",
		"lint": "pnpm recursive run lint",
		"typecheck": "pnpm recursive run typecheck",
		"dev:frontend": "pnpm dev --filter=frontend",
